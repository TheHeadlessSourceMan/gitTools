    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "run gitRecursive",
            "type": "debugpy",
            "request": "launch",
            "program": "gitRecursive.py",
            "console": "integratedTerminal",
            "args": [
                "--fetch","--sync","d:\\python"
            ]
        }
    ]
}